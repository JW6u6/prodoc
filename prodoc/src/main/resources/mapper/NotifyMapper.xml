<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="com.prodoc.notify.mapper.NotifyMapper">
  
  	<!-- 타겟 지정 알림 프로시저 -->
  	<insert id="targetNotify" parameterType="NotifyVO" statementType="CALLABLE" >
  		{call TARGET_NOTIFY(
	 								#{reciveUser}
	 								, #{noteType}
	 								, #{displayId}
	 								, #{result, mode=OUT, jdbcType=VARCHAR, javaType=string}
 								)}
  	</insert>
  
  </mapper>